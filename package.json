        "mac": "cmd+alt+s",
        "when": "mdb.isPlayground == true"
      },
      {
        "command": "mdb.runAllPlaygroundBlocks",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "mdb.isPlayground == true"
      },
      {
        "command": "mdb.saveMongoDBDocument",
        "key": "ctrl+s",
        "mac": "cmd+s",
        "when": "resourceScheme == VIEW_DOCUMENT_SCHEME"
      }
    ],
    "capabilities": {
